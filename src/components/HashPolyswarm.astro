---
const { apiID, dbSource } = Astro.props;
---

<div id={dbSource} class="hidden">
    <h4 class="mt-4 text-lg font-bold">Información general</h4>
    <p
        id=`api${apiID}-result-${dbSource}-extfiletype`
        class="text-gray-300 text-base"
    >
        <strong>Detección como:</strong>
    </p>
    <p
        id=`api${apiID}-result-${dbSource}-malwarefamliy`
        class="text-gray-300 text-base"
    >
        <strong>Familia de malware:</strong>
    </p>
</div>
