<article
    id=`vthash`
    class="max-w-sm p-6 bg-purple-800/30 border border-gray-400/50 rounded-lg shadow hidden"
>
    <h3 id=`vthash-title` class="mb-2 text-2xl font-bold text-white"></h3>
    <h4 class="mt-4 text-lg font-bold">Información general</h4>
    <ul class="text-gray-300">
        <li>
            <strong>Reputación: </strong><span
                id=`vthash-reputation`
                class="text-base font-normal"></span>
        </li>
        <li>
            <strong>Tipo de archivo: </strong><span
                id=`vthash-typefile`
                class="text-base font-normal"></span>
        </li>
        <li>
            <strong>Detección como: </strong><span
                id=`vthash-magicnumber`
                class="text-base font-normal"></span>
        </li>
    </ul>
    <h4 class="mt-4 text-lg font-bold">Detecciones por vendors de seguridad</h4>
    <ul class="text-gray-300">
        <li>
            <strong
                >Inofensivo: <span
                    id=`vthash-analysis-harmless`
                    class="text-base font-normal"></span></strong
            >
        </li>
        <li>
            <strong
                >Malicioso: <span
                    id=`vthash-analysis-malicious`
                    class="text-base font-normal"></span></strong
            >
        </li>
        <li>
            <strong
                >Sospechoso: <span
                    id=`vthash-analysis-suspicious`
                    class="text-base font-normal"></span></strong
            >
        </li>
        <li>
            <strong
                >No hay información: <span
                    id=`vthash-analysis-undetected`
                    class="text-base font-normal"></span></strong
            >
        </li>
        <li>
            <strong
                >No hubo respuesta: <span
                    id=`vthash-analysis-timeout`
                    class="text-base font-normal"></span></strong
            >
        </li>
    </ul>
    <h4 class="mt-4 text-lg font-bold">Votos de reputación</h4>
    <ul class="text-gray-300">
        <li>
            <strong
                >Inofensivo: <span
                    id=`vthash-votes-harmless`
                    class="text-base font-normal"></span></strong
            >
        </li>
        <li>
            <strong
                >Malicioso: <span
                    id=`vthash-votes-malicious`
                    class="text-base font-normal"></span></strong
            >
        </li>
    </ul>
</article>
